[application]

config/name="20 . 01. Grid Game"
run/main_scene="res://Game.tscn"
config/icon="res://icon.png"

[autoload]

ForthUtil="*res://Autoloads/ForthUtil.gd"

[debug]

gdscript/warnings/shadowed_variable=false

[display]

window/size/width=600
window/size/height=1200
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

clear=false

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom_font="res://kenney_mini.tres"

[input]

up={
"deadzone": 0.5,
"events": [  ]
}
down={
"deadzone": 0.5,
"events": [  ]
}
left={
"deadzone": 0.5,
"events": [  ]
}
right={
"deadzone": 0.5,
"events": [  ]
}

[network]

limits/debugger_stdout/max_chars_per_second=100000

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.176471, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
